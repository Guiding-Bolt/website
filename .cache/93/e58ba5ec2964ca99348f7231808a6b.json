ng: 4px 8px;\n    font-size: 1rem;\n    filter: drop-shadow(0 0 4px rgba(255, 0, 0, 0.7));\n\n    &::before {\n      content: 'â€¢';\n      display: inline-block;\n      padding-right: 6px;\n    }\n  }\n\n  .live-dot {\n    display: inline-block;\n    height: 14px;\n    width: 14px;\n    border-radius: 50%;\n    background: red;\n  }\n}\n\n.h-text {\n  margin: 0;\n  font-size: 2em;\n  max-width: 34ch;\n  font-weight: 200;\n  letter-spacing: 1px;\n\n  em {\n    font-style: normal;\n    font-weight: 600;\n  }\n}\n\n.h-button {\n  border: none;\n  outline: none;\n  display: block;\n  padding: 10px 12px;\n  font-size: 1.7em;\n  font-weight: 600;\n  text-decoration: none;\n  text-align: center;\n  cursor: pointer;\n  user-select: none;\n  background: const.$text;\n  color: const.$background;\n\n  &.highlight {\n    background: const.$highlight;\n    color: const.$text;\n  }\n\n  &:hover {\n    filter: brightness(105%);\n  }\n\n  &:active {\n    filter: none;\n  }\n}\n","_typography.scss":"@use 'styles/const';\n\n@import url('https://fonts.googleapis.com/css2?family=Barlow:wght@200;300;400;600&display=swap');\n\n* {\n  font-family: 'Barlow', sans-serif;\n}\n\nh1,\nh2,\nh3,\nh4,\nh5 {\n  margin: 0;\n  font-weight: 600;\n}\n\nh1 {\n  font-size: 5em;\n}\n\na {\n  color: const.$text;\n  font-weight: 600;\n  opacity: 0.9;\n\n  &:hover {\n    opacity: 1;\n  }\n}\n","_const.scss":"$background: var(--background, #454545);\n$highlight: var(--highlight, #ff2c2c);\n$text: var(--background, #fbfbfb);\n","_view_homepage.scss":"@use 'styles/const';\n@use 'styles/typography';\n\n.homepage-container {\n  height: 100vh;\n  overflow-y: scroll;\n}\n\n.homepage-banner {\n  min-height: 100vh;\n  height: fit-content;\n  width: min(100%, 1080px);\n  margin: auto;\n  display: grid;\n  grid-template-rows: 20vh 200px auto 10vh;\n  grid-template-columns: 25px 2fr 25px 1fr 25px;\n\n  .logo {\n    grid-row: 2;\n    grid-column: 2 / span 3;\n\n    font-size: 8.5em;\n  }\n\n  .description {\n    grid-row: 3 / span 2;\n    grid-column: 2 / span 1;\n\n    a {\n      color: const.$highlight;\n    }\n  }\n\n  .about-link {\n    display: block;\n    margin-top: 10vh;\n  }\n\n  .links {\n    grid-row: 3 / span 2;\n    grid-column: 4 / span 1;\n\n    .h-button + .h-button {\n      margin-top: 1em;\n    }\n\n    img {\n      height: 100px;\n      width: 100px;\n    }\n  }\n\n  @media screen and (max-width: 750px) {\n    grid-template-columns: 15px auto 15px;\n    grid-template-rows: 5vh 225px 100px auto 10vh;\n    min-height: 100vh;\n    width: 100vw;\n\n    .logo {\n      grid-row: 2;\n      grid-column: 2;\n      font-size: max(8vw, 8em);\n    }\n    .links {\n      grid-row: 3;\n      grid-column: 2;\n\n      display: flex;\n      height: 52px;\n      width: 100%;\n\n      .h-button {\n        width: 50%;\n      }\n      .h-button + .h-button {\n        margin-top: 0;\n        margin-left: 1em;\n      }\n    }\n    .description {\n      grid-row: 4 / span 1;\n      grid-column: 2;\n    }\n  }\n\n  @media screen and (max-width: 600px) {\n    grid-template-rows: 5vh fit-content(200px) 5vh 10vh auto 10vh;\n\n    .logo {\n      font-size: 18vw;\n    }\n\n    .links {\n      grid-row: 4;\n    }\n\n    .description {\n      grid-row: 5;\n    }\n  }\n\n  @media screen and (max-width: 500px) {\n    grid-template-rows: 5vh fit-content(200px) 5vh 20vh auto 10vh;\n\n    .links {\n      width: calc(100% - 30px);\n      flex-direction: column;\n\n      .h-button {\n        width: 100%;\n      }\n      .h-button + .h-button {\n        margin-left: 0;\n        margin-top: 10px;\n      }\n    }\n  }\n\n  @media screen and (max-width: 305px) {\n    .logo {\n      position: relative;\n\n      .live {\n        position: absolute;\n        bottom: -25px;\n        right: 0;\n      }\n    }\n  }\n}\n\n.audience-model {\n  width: 400px;\n  display: grid;\n  grid-template-columns: repeat(3, 33%);\n  grid-template-rows: 140px 140px;\n  gap: 10px 0px;\n  justify-items: center;\n\n  img {\n    width: 100px;\n    grid-row: 2;\n\n    &:nth-of-type(3) {\n      margin-top: 25px;\n    }\n\n    &.host {\n      width: 140px;\n      height: 140px;\n      grid-row: 1;\n      grid-column: 2;\n      margin: 0 auto;\n    }\n  }\n}\n","global.scss":"@forward 'styles/typography';\n@forward 'styles/branding';\n\n@use 'styles/const';\n@use 'styles/view_homepage';\n\n:root {\n  scroll-behavior: smooth;\n}\n\nbody {\n  margin: 0;\n  background: const.$background;\n  color: const.$text;\n}\n"},"lineCount":null}},"error":null,"hash":"0395491e25fcc63c2f2a68118a883f86","cacheData":{"env":{}}}