m;\n  line-height: 1em;\n\n  // &::before {\n  //   content: '';\n  //   position: absolute;\n  //   height: 0.1em;\n  //   background: const.$highlight;\n  //   top: 0;\n  //   left: 0;\n\n  //   --width: 3.5em;\n  //   animation: expand-width;\n  //   animation-duration: 800ms;\n  //   animation-fill-mode: forwards;\n  // }\n  &::after {\n    content: '';\n    position: absolute;\n    height: 0.1em;\n    background: const.$highlight;\n    bottom: 36px;\n    left: 0;\n\n    --width: 2em;\n    animation: expand-width;\n    animation-duration: 800ms;\n    animation-fill-mode: forwards;\n  }\n\n  &.small {\n    font-weight: 400;\n  }\n}\n\n.gb-text {\n  margin: 0;\n  font-size: 2.5em;\n  max-width: 34ch;\n  font-weight: 200;\n  letter-spacing: 1px;\n\n  em {\n    font-style: normal;\n    color: const.$highlight;\n  }\n}\n\n.gb-button {\n  border: none;\n  outline: none;\n  display: block;\n  padding: 10px 12px;\n  font-size: 1.7em;\n  font-weight: 600;\n  text-decoration: none;\n  text-align: center;\n  cursor: pointer;\n  user-select: none;\n  background: const.$text;\n  color: const.$background;\n\n  &.highlight {\n    background: const.$highlight;\n    color: const.$text;\n  }\n\n  &:hover {\n    filter: brightness(105%);\n  }\n\n  &:active {\n    filter: none;\n  }\n}\n","_keyframes.scss":"@keyframes expand-width {\n  from {\n    width: 0;\n  }\n  to {\n    width: var(--width, 100%);\n  }\n}\n","_view_homepage.scss":"@use 'styles/const';\n@use 'styles/typography';\n\n.homepage-container {\n  height: 100vh;\n  overflow-y: scroll;\n}\n\n.homepage-banner {\n  min-height: 100vh;\n  height: fit-content;\n  width: min(100%, 1280px);\n  margin: auto;\n  display: grid;\n  grid-template-rows: 20vh 220px fit-content(10px) auto 10vh;\n  grid-template-columns: 25px 1fr 10vw 1fr 25px;\n\n  .company_name {\n    grid-row: 2;\n    grid-column: 4 / span 1;\n\n    font-size: 4.5em;\n  }\n\n  .description {\n    grid-row: 3 / span 1;\n    grid-column: 4 / span 1;\n\n    a {\n      color: const.$highlight;\n    }\n  }\n\n  @media screen and (max-width: 1210px) {\n    display: grid;\n    padding: 0px 15px;\n    grid-template-columns: auto minmax(calc(100vw - 30px), 660px) auto;\n    grid-template-rows: 5vh auto 5vh fit-content(150px) 5vh auto 10vh;\n    flex-direction: column;\n    align-items: center;\n    width: calc(100vw - 34px);\n\n    .company_name {\n      grid-row: 2;\n      grid-column: 2;\n      padding-bottom: 10px;\n      &::after {\n        bottom: 0;\n      }\n    }\n\n    .description {\n      grid-row: 4;\n      grid-column: 2;\n    }\n  }\n}\n\n.homepage-products {\n  grid-row: 2 / span 2;\n  grid-column: 2 / span 1;\n  align-self: center;\n  display: flex;\n  flex-direction: column;\n  @media screen and (max-width: 1210px) {\n    grid-row: 6;\n  }\n}\n\n.homepage-product-preview {\n  height: 54px;\n  padding: 15px;\n  color: const.$text;\n  border: 4px solid;\n  border-radius: 10px;\n  transition: all 250ms ease-in-out;\n  cursor: pointer;\n  display: flex;\n  justify-content: flex-end;\n  flex-direction: column;\n  flex-wrap: wrap;\n\n  .logo {\n    height: 54px;\n    width: 54px;\n    border-radius: 10px;\n  }\n\n  .name {\n    font-weight: 400;\n    font-size: 1.5em;\n    width: min-content;\n\n    & + .property {\n      margin-top: 6px;\n    }\n  }\n\n  .property {\n    font-weight: 400;\n    font-size: 1.2em;\n    white-space: nowrap;\n    width: min-content;\n\n    & + & {\n      margin-top: 6px;\n    }\n  }\n\n  &:hover {\n    color: const.$highlight;\n  }\n\n  & + & {\n    margin-top: 20px;\n  }\n\n  @media screen and (max-width: 1210px) {\n    height: auto;\n    align-items: center;\n\n    .logo {\n      margin-bottom: 10px;\n    }\n\n    .property + .property {\n      margin-top: 10px;\n    }\n  }\n}\n","global.scss":"@forward 'styles/typography';\n@forward 'styles/branding';\n@forward 'styles/keyframes';\n\n@use 'styles/const';\n@use 'styles/view_homepage';\n\n:root {\n  scroll-behavior: smooth;\n}\n\nbody {\n  margin: 0;\n  background: const.$background;\n  color: const.$text;\n}\n"},"lineCount":null}},"error":null,"hash":"7592e40912218c1e841a9047134ca591","cacheData":{"env":{}}}